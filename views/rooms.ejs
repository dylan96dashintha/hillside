<body>

	
	
	<div id="featured-hotel" class="fh5co-bg-color">
		<div class="container">
			
			<div class="row">
				<div class="col-md-12">
					<div class="section-title text-center">
						<h12>Available Rooms</h2>
					</div>
				</div>
			</div>

			<div class="row">
				<% for (var i=0; i<((roomDetails.length) ); i+=3){ %> 
					<%if(i<roomDetails.length){%>
				<div class="feature-full-1col">
					<div class="image" style="background-image: url(images/<%=roomDetails[i].image%>);">
						<div class="descrip text-center">
							<p><small>For as low as</small><span>Rs.<%=roomDetails[i].price%>/Night</span></p>
						</div>
					</div>
					<div class="desc">
						<h3><%=roomDetails[i].description%></h3>
						<p><%=roomDetails[i].fullDescription%></p>
						<p><a href="#" class="btn btn-primary btn-luxe-primary">Book Now <i class="ti-angle-right"></i></a></p>
					</div>
				</div><%}%>
				<div class="feature-full-2col">
					<%if(i+1<roomDetails.length){%>
					<div class="f-hotel"> 
						<div class="image" style="background-image: url(images/<%=roomDetails[i+1].image%>);">
							<div class="descrip text-center">
								<p><small>For as low as</small><span>Rs.<%=roomDetails[i+1].price%>/Night</span></p>
							</div>
						</div>
						<div class="desc"> 
							<h3><%=roomDetails[i+1].description%></h3>
							<p><%=roomDetails[i+1].fullDescription%></p>
							<p><a href="#" class="btn btn-primary btn-luxe-primary">Book Now <i class="ti-angle-right"></i></a></p>
						</div>
					</div>
					<%}%>
					<%if(i+2<roomDetails.length){%>
					<div class="f-hotel">
						<div class="image" style="background-image: url(images/<%=roomDetails[i+2].image%>);">
							<div class="descrip text-center">
								<p><small>For as low as</small><span>Rs.<%=roomDetails[i+2].price%>/Night</span></p>
							</div>
						</div>
						<div class="desc">
							<h3><%=roomDetails[i+2].description%></h3>
							<p><%=roomDetails[i+2].fullDescription%></p>
							<p><a href="#" class="btn btn-primary btn-luxe-primary">Book Now <i class="ti-angle-right"></i></a></p>
						</div>
					</div>
					<%}%>
				</div>
				<%}%>
			</div>

		</div>
	</div>

	
